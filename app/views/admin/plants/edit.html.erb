<h2>植物情報の編集</h2>
<%= form_with model: @plant, url: admin_plant_path do |f| %>
  <label for="inputName">植物名</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.text_field :genre, placeholder: "プロフィール画像" %>
  <%= f.submit "変更を保存" %>
<% end %>